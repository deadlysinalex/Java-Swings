    }                                           

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         float num1, num2, result;//addition
        num1=Float.parseFloat(jTextField1.getText());
        num2=Float.parseFloat(jTextField2.getText());
        result=num1+ num2;
        jLabel5.setText("");
        jLabel4.setText(String.valueOf(result));
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         float num1, num2, result;//Subtraction+
        num1=Float.parseFloat(jTextField1.getText());
        num2=Float.parseFloat(jTextField2.getText());
        result=num1-num2;
        jLabel5.setText("");
        jLabel4.setText(String.valueOf(result));
    }                                        

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         float num1, num2, result;//Multiplication
        num1=Float.parseFloat(jTextField1.getText());
        num2=Float.parseFloat(jTextField2.getText());
        result=num1*num2;
        jLabel5.setText("");
        jLabel4.setText(String.valueOf(result));
    }                                        

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
 float num1, num2, result;//Divistion
        num1=Float.parseFloat(jTextField1.getText());
        num2=Float.parseFloat(jTextField2.getText());
        result=num1/num2;
        jLabel4.setText(String.valueOf(result));
        jLabel5.setText("");
    }                                        

    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
      
         double num1,Pi,result;
        num1=Double.parseDouble(jTextField1.getText());
        
        Pi = 3.1415;
        result=num1*Pi;
        jLabel4.setText(String.valueOf(result));
        jLabel5.setText("");
    }                                        

    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here: Area Of Triangle (base*hight)/2
        double num1,num2,result;
        num1=Double.parseDouble(jTextField1.getText());
        
        num2 = Double.parseDouble(jTextField2.getText());
        result=(num1*num2)/2;
        jLabel4.setText(String.valueOf(result));
        jLabel5.setText("");
    }                                        

    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        double num1,num2,result1, result2;
        num1=Double.parseDouble(jTextField1.getText());
        
        num2 = Double.parseDouble(jTextField2.getText());
        result1=num1*num1;
        result2=num2*num2;
        jLabel4.setText(String.valueOf(result1));
        jLabel5.setText(String.valueOf(result2));
    }                                         

    private void jButton11ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        double num1,num2,result1, result2;
        num1=Double.parseDouble(jTextField1.getText());
        
        num2 = Double.parseDouble(jTextField2.getText());
        result1=num1*num1*num1;
        result2=num2*num2*num2;
        jLabel4.setText(String.valueOf(result1));
        jLabel5.setText(String.valueOf(result2));
    }                                         

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        double num1,num2,result;
        num1=Double.parseDouble(jTextField1.getText());
        
        num2 = Double.parseDouble(jTextField2.getText());
        result=num1*num2;
        jLabel4.setText(String.valueOf(result));
        jLabel5.setText("");
    }                                        

    private void jToggleButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                               
        // TODO add your handling code here:
        
                double num1,num2,result1, result2;
        num1=Double.parseDouble(jTextField1.getText());
        
        num2 = Double.parseDouble(jTextField2.getText());
        result1=Math.sqrt(num1);
        result2=Math.sqrt(num2);
        jLabel4.setText(String.valueOf(result1));
        jLabel5.setText(String.valueOf(result2));
    }                                              

    private void jButton12ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
         double num1,num2,result1, result2;
        num1=Double.parseDouble(jTextField1.getText());
        
        num2 = Double.parseDouble(jTextField2.getText());
        result1=Math.cbrt(num1);
        result2=Math.cbrt(num2);
        jLabel4.setText(String.valueOf(result1));
        jLabel5.setText(String.valueOf(result2));
    
    }                                         

